@model Employee
<div class="col-md-6 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Create Employee</h4>
                  <form class="forms-sample" method="post">
                    <div class="form-group">
                      <label asp-for="FullName"></label>
                      <input asp-for="FullName" class="form-control">
                      <span asp-validation-for="FullName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                      <label asp-for="Position"></label>
                      <input asp-for="Position" class="form-control">
                      <span asp-validation-for="Position" class="text-danger"></span>

                    </div>
                    <div class="form-group">
                      <label asp-for="ImgUrl">ImgUrl</label>
                      <input asp-for="ImgUrl" class="form-control" >
                      <span asp-validation-for="ImgUrl" class="text-danger"></span>

                    </div>
                    <div class="form-group">
                      <label asp-for="Testimonial"></label>
                      <input asp-for="Testimonial" class="form-control">
                      <span asp-validation-for="Testimonial" class="text-danger"></span>

                    </div>
                    <button type="submit" class="btn btn-primary me-2">Create</button>
                    <button class="btn btn-light" asp-area="Manage" asp-controller="Employee" asp-action="Index">Cancel</button>
                  </form>
                </div>
              </div>
            </div>




